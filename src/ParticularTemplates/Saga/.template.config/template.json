{
  "$schema": "http://json.schemastore.org/template",
  "author": "Particular Software",
  "classifications": [
    "NServiceBus",
    "Code",
    "General"
  ],
  "name": "NServiceBus Saga",
  "identity": "NServiceBus.Saga",
  "shortName": "nsbsaga",
  "tags": {
    "language": "C#",
    "type": "item"
  },
  "sourceName": "SagaName",
  "defaultName": "NewSaga1",
  "primaryOutputs": [
    {
      "condition": "(HostIdentifier != \"dotnetcli\" && HostIdentifier != \"dotnetcli-preview\")",
      "path": "SagaName.cs"
    }
  ],
  "postActions": [
    {
      "id": "openInEditor",
      "condition": "(HostIdentifier != \"dotnetcli\" && HostIdentifier != \"dotnetcli-preview\")",
      "description": "Opens SagaName.cs in the editor.",
      "manualInstructions": [],
      "actionId": "84C0DA21-51C8-4541-9940-6CA19AF04EE6",
      "args": {
        "files": "0"
      },
      "continueOnError": true
    }
  ],
  "symbols": {
    "defaultNamespace": {
      "type": "bind",
      "binding": "msbuild:RootNamespace",
      "replaces": "NamespaceName",
      "defaultValue": "Handlers"
    },
    "BindImplicitUsings": {
      "type": "bind",
      "binding": "msbuild:ImplicitUsings",
      "defaultValue": "false"
    },
    "HostIdentifier": {
      "type": "bind",
      "binding": "HostIdentifier"
    },
    "ImplicitUsings": {
      "type": "computed",
      "datatype": "bool",
      "value": "(BindImplicitUsings == \"true\" || BindImplicitUsings == \"enable\")"
    },
    "messagetype1": {
      "type": "parameter",
      "description": "The first message type that will be handled by the saga.",
      "datatype": "string",
      "defaultValue": "MessageType1",
      "replaces": "MessageType1"
    },
    "messagetype2": {
      "type": "parameter",
      "description": "The second message type that will be handled by the saga.",
      "datatype": "string",
      "defaultValue": "MessageType2",
      "replaces": "MessageType2"
    }
  }
}