{
  "$schema": "http://json.schemastore.org/template",
  "author": "Particular Software",
  "classifications": [
    "NServiceBus",
    "Code",
    "General"
  ],
  "name": "NServiceBus Message Handler",
  "identity": "NServiceBus.Handler",
  "shortName": "nsbhandler",
  "tags": {
    "language": "C#",
    "type": "item"
  },
  "sourceName": "ClassName",
  "defaultName": "NewHandler1",
  "symbols": {
    "defaultNamespace": {
      "type": "bind",
      "binding": "msbuild:RootNamespace",
      "replaces": "NamespaceName",
      "defaultValue": "Handlers"
    },
    "BindImplicitUsings": {
      "type": "bind",
      "binding": "msbuild:ImplicitUsings"
    },
    "ImplicitUsings": {
      "type": "computed",
      "datatype": "bool",
      "value": "(BindImplicitUsings == \"true\" || BindImplicitUsings == \"enable\")"
    },
    "messagetype": {
      "type": "parameter",
      "description": "The type of message that will be handled by the message handler.",
      "datatype": "string",
      "replaces": "MessageType"
    }
  }
}